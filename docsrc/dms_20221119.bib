
@article{armi_maximal_1986,
	title = {Maximal two-layer exchange through a contraction with barotropic net flow},
	volume = {164},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/product/identifier/S0022112086002458/type/journal_article},
	doi = {10.1017/S0022112086002458},
	abstract = {The gravitational exchange of two fluids with different densities between reservoirs connected by a channel of constant depth and slowly varying breadth is analysed as a problem of internal hydraulics. It is shown that maximal two-way exchange with a net barotropic flow requires the presence of two controls, one at the narrrowest section and a second or ‘virtual’ control lying to one side of the narrowest section. The two controls are connected by a subcritical region, but are separated from subcritical conditions in the reservoirs by supercritical flow and stationary internal bores. Solutions are found for maximal exchange without a net barotropic component, in which case the problem is similar to that first examined by Stommel \& Farmer (1953). The Stommel \& Farmer analysis is shown to be a rather special limiting example of submaximal exchange, not generally applicable to natural flows. The addition of a net barotropic flow yields a range of different flow types, including maximal exchange, one-layer baroclinic flow, one-layer barotropic flow, submaximal flow governed by a reservoir condition and two-layer unidirectional flow. The maximal-exchange solution is integrated for periodic barotropic flow.},
	pages = {27--51},
	journaltitle = {Journal of Fluid Mechanics},
	shortjournal = {J. Fluid Mech.},
	author = {Armi, L. and Farmer, D. M.},
	urldate = {2022-11-03},
	date = {1986-03},
	langid = {english},
	file = {Armi and Farmer_1986_Maximal two-layer exchange through a contraction with barotropic.pdf:C\:\\Users\\eli\\Zotero\\storage\\IL2VGJ76\\Armi and Farmer_1986_Maximal two-layer exchange through a contraction with barotropic.pdf:application/pdf},
}

@article{armi_hydraulics_1986,
	title = {The hydraulics of two flowing layers with different densities},
	volume = {163},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/product/identifier/S0022112086002197/type/journal_article},
	doi = {10.1017/S0022112086002197},
	abstract = {This is a theoretical and experimental study of the basic hydraulics of two flowing layers. Unlike single-layer flows, two-layer flows respond quite differently to bottom depth as opposed to width variations. Bottom-depth changes affect the lower layer directly and the upper layer only indirectly. Changes in width can affect both layers. In fact for flows through a contraction control two distinct flow configurations are possible; which one actually occurs depends on the requirements of matching a downstream flow. Two-layer flows can pass through internally critical conditions at other than the narrowest section. When the two layers are flowing in the same direction, the result is a strong coupling between the two layers in the neighbourhood of the control. For contractions a particularly simple flow then exists upstream in which there is no longer any significant interfacial dynamics; downstream in the divergent section the flow remains internally supercritical, causing one of the layers to be rapidly accelerated with a resulting instability at the interface. A brief discussion of internal hydraulic jumps based upon the energy equations as opposed to the more traditional momentum equations is included. Previous uniqueness problems are thereby avoided.},
	pages = {27--58},
	journaltitle = {Journal of Fluid Mechanics},
	shortjournal = {J. Fluid Mech.},
	author = {Armi, Laurence},
	urldate = {2022-11-03},
	date = {1986-02},
	langid = {english},
	file = {Armi_1986_The hydraulics of two flowing layers with different densities.pdf:C\:\\Users\\eli\\Zotero\\storage\\E56F554Q\\Armi_1986_The hydraulics of two flowing layers with different densities.pdf:application/pdf},
}

