	issn = {03781909},
	doi = {10.1007/s10641-013-0186-7},
	abstract = {Drift-feeding models are essential components of broader models that{\textbackslash}nlink stream habitat to salmonid populations and community dynamics. But{\textbackslash}nis an additional feeding mode needed for understanding and predicting{\textbackslash}nsalmonid population responses to streamflow and other environmental{\textbackslash}nfactors? We addressed this question by applying two versions of the{\textbackslash}nindividual-based model {inSTREAM} to a field experiment in which{\textbackslash}nstreamflow was varied in experimental units that each contained a stream{\textbackslash}npool and the adjacent upstream riffle. The two model versions differed{\textbackslash}nonly in the feeding options available to fish. Both versions of {inSTREAM}{\textbackslash}nincluded drift feeding; one also included a search feeding mode to{\textbackslash}nrepresent feeding in which food availability is largely independent of{\textbackslash}nstreamflow, such as feeding from the benthos, or feeding from the water{\textbackslash}ncolumn or the water's surface in low water velocities. We compared the{\textbackslash}nabilities of the two model versions to fit the observed distributions of{\textbackslash}ngrowth by individual rainbow trout (Oncorhynchus mykiss) in the field{\textbackslash}nexperiment. The version giving fish the daily choice between drift or{\textbackslash}nsearch feeding better fit observations than the version in which fish{\textbackslash}nfed only on drift. Values for drift and search food availability from{\textbackslash}ncalibration to the individual mass changes of fish in experimental units{\textbackslash}nwith unaltered streamflow yielded realistic distributions of individual{\textbackslash}ngrowth when applied to experimental units in which streamflow was{\textbackslash}nreduced by 80 \%. These results correspond with empirical studies that{\textbackslash}nshow search feeding can be an important alternative to drift feeding for{\textbackslash}nsalmonids in some settings, and indicate that relatively simple{\textbackslash}nformulations of both processes in individual-based population models can{\textbackslash}nbe useful in predicting the effects of environmental alterations on fish{\textbackslash}npopulations.},
	pages = {615--625},
	number = {5},
	journaltitle = {Environmental Biology of Fishes},
	author = {Harvey, Bret C. and Railsback, Steven F.},
	date = {2014},
	note = {{ISBN}: 0378-1909},
	keywords = {Drift, Foraging mode, Individual-based modeling, Salmonidae, Search feeding},
	file = {Harvey and Railsback_2014_Feeding modes in stream salmon.pdf:C\:\\Users\\eli\\Zotero\\storage\\5RL4Q5RK\\Harvey and Railsback_2014_Feeding modes in stream salmon.pdf:application/pdf},
}

