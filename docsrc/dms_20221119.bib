	langid = {english},
	file = {Weinkauf et al._2011_Stable Feature Flow Fields.pdf:C\:\\Users\\eli\\Zotero\\storage\\C8SHX8XP\\Weinkauf et al._2011_Stable Feature Flow Fields.pdf:application/pdf},
}

