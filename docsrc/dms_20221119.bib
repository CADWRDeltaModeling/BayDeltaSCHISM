	file = {Merwade_2009_Effect of spatial trends on interpolation of river bathymetry.pdf:C\:\\Users\\eli\\Zotero\\storage\\QX6FGBCZ\\Merwade_2009_Effect of spatial trends on interpolation of river bathymetry.pdf:application/pdf},
}

