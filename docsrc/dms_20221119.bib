@article{holland_internal_2002,
	title = {Internal hydraulic jumps and mixing in two-layer flows},
	volume = {470},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/product/identifier/S002211200200188X/type/journal_article},
	doi = {10.1017/S002211200200188X},
	abstract = {Internal hydraulic jumps in two-layer flows are studied, with particular emphasis 
on their role in entrainment and mixing. For highly entraining internal jumps, a 
new closure is proposed for the jump conditions. The closure is based on two main 
assumptions: (i) most of the energy dissipated at the jump goes into turbulence, 
and (ii) the amount of turbulent energy that a stably stratified flow may contain 
without immediately mixing further is bounded by a measure of the stratification. As 
a consequence of this closure, surprising bounds emerge, for example on the amount 
of entrainment that may take place at the location of the jump. These bounds are 
probably almost achieved by highly entraining internal jumps, such as those likely to 
develop in dense oceanic over flows. The values obtained here are in good agreement 
with the existing observations of the spatial development of oceanic downslope 
currents, which play a crucial role in the formation of abyssal and intermediate 
waters in the global ocean.},
	pages = {63--83},
	journaltitle = {Journal of Fluid Mechanics},
	shortjournal = {J. Fluid Mech.},
	author = {Holland, David M. and Rosales, Rodolfo R. and Stefanica, Dan and Tabak, Esteban G.},
	urldate = {2022-11-03},
	date = {2002-11-10},
	langid = {english},
	file = {Holland et al._2002_Internal hydraulic jumps and mixing in two-layer flows.pdf:C\:\\Users\\eli\\Zotero\\storage\\SSD8MVN3\\Holland et al._2002_Internal hydraulic jumps and mixing in two-layer flows.pdf:application/pdf},
}

@article{holleman_numerical_2013,
	title = {Numerical diffusion for flow-aligned unstructured grids with application to estuarine modeling: {FLOW}-{ALIGNED} {UNSTRUCTURED} {GRIDS}},
	volume = {72},
	issn = {02712091},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/fld.3774},
