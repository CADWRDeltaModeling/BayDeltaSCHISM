	file = {Lerczak et al._2009_The Temporal Response of the Length of a Partially Stratified Es.pdf:C\:\\Users\\eli\\Zotero\\storage\\HIKPZKW3\\Lerczak et al._2009_The Temporal Response of the Length of a Partially Stratified Es.pdf:application/pdf},
