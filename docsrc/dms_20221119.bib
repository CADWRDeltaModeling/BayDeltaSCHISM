	file = {Rippeth et al._2002_Reynolds Stress and Turbulent Energy Production in a Tidal Chann.pdf:C\:\\Users\\eli\\Zotero\\storage\\K44V6Z4M\\Rippeth et al._2002_Reynolds Stress and Turbulent Energy Production in a Tidal Chann.pdf:application/pdf},
