	keywords = {Anadromous salmonids, Aquatic habitat, Effectiveness monitoring, Lotic productivity},
	file = {Bryant et al._2005_An approach to effectiveness m.pdf:C\:\\Users\\eli\\Zotero\\storage\\ECHBLY3K\\Bryant et al._2005_An approach to effectiveness m.pdf:application/pdf},
