	volume = {143},
	issn = {0002-8487},
	url = {http://www.tandfonline.com/doi/abs/10.1080/00028487.2014.880740},
	doi = {10.1080/00028487.2014.880740},
	abstract = {We examined the contribution of three types of side channels based on{\textbackslash}ntheir hydrologic connectivity (seasonally disconnected, partially{\textbackslash}nconnected, and connected) to production of juvenile anadromous{\textbackslash}nsalmonids. Juvenile steelhead Oncorhynchus mykiss and Chinook Salmon O.{\textbackslash}ntshawytscha were found in all three of these side channel types and in{\textbackslash}neach year of the study. Upon connection with the main stem at high{\textbackslash}nflows, the seasonally disconnected side channels experienced an emptying{\textbackslash}nout of the previous year's fish while filling with young-of-year fish{\textbackslash}nduring the 2- to 4-month period of hydrologic connection. There were no{\textbackslash}ndifferences between the densities of juvenile steelhead and Chinook{\textbackslash}{nSalmon} and the rate of smolts produced among the three types of side{\textbackslash}nchannels. Recently reintroduced Coho Salmon O. kisutch had sporadic{\textbackslash}npresence and abundance in partially and connected side channels, but the{\textbackslash}nsmolt production rate was over two times that of steelhead and Chinook{\textbackslash}{nSalmon} in seasonally disconnected side channels. Within seasonally{\textbackslash}ndisconnected side channels, young-of-year salmonids in deep pools ({\textgreater}={\textbackslash}n100cm) had greater survival than those in shallow pools ({\textless}100cm).{\textbackslash}{nDensities} of juvenile steelhead in all side channel types were similar{\textbackslash}nto those in tributaries and were higher than in main-stem lateral{\textbackslash}nmargins. Juvenile Chinook Salmon densities were higher in side channels{\textbackslash}nthan in both tributary and main-stem lateral margins. Our results{\textbackslash}nsuggest that improving quality of pool habitat within seasonally{\textbackslash}ndisconnected side channels can result in improved survival for juvenile{\textbackslash}nanadromous salmonids during the period of disconnection. Habitat{\textbackslash}nimprovement in these seasonally disconnected side channels should be{\textbackslash}nrecognized as a worthy restoration strategy, especially when full{\textbackslash}nconnectivity of side channels may not be a feasible target (e.g.,{\textbackslash}nthrough lack of water availability) or when full connectivity may{\textbackslash}npresent too high a risk (e.g., flooding, stream capture, bank{\textbackslash}ndestabilization). Received August 30, 2013; accepted December 9, 2013},
	pages = {757--767},
	number = {3},
	journaltitle = {Transactions of the American Fisheries Society},
	author = {Martens, Kyle D. and Connolly, Patrick J.},
	date = {2014},
	note = {{ISBN}: 0002-8487},
	file = {Martens and Connolly_2014_Juvenile Anadromous Salmonid P.pdf:C\:\\Users\\eli\\Zotero\\storage\\L7K8DU2E\\Martens and Connolly_2014_Juvenile Anadromous Salmonid P.pdf:application/pdf},
}

@article{Mattheus2009,
